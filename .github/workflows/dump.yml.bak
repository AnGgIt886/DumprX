name: Update Gradle wrapper

on:
  push:

jobs:
  update:
    runs-on: ubuntu-latest
    permissions: write-all
    steps:
      - name: Checkout
        uses: actions/checkout@v5
      - name: Setup
        run: bash setup.sh
      - name: Dump Firmware
        run: bash dumper.sh https://miuirom.org/id/halaman-unduh-file?o=redmi-9t&v=14.0.5.0.SJQMIXM&l=id&t=Recovery